model User {
  id             String       @id @default(uuid())
  name           String
  email          String       @unique
  role           Role         @default(USER)
  password       String?
  token          String?
  lastLogin      DateTime     @default(now())
  createdAt      DateTime     @default(now())
  favorites      Supplement[]
  googleId       String?      @unique
  googleAuthCode String?
  googleAuthExp  DateTime?
}

enum Role {
  ADMIN
  USER
}
